<div class="row">
  <% @photos_low_res.each do |photo| %>
    <div class="col-sm-6 col-md-4 col-lg-3 mb-2">
      <div class="card" style="background-image: url('<%= photo_url(photo) %>'); background-position: center center; background-repeat: no-repeat; background-size: cover; min-height:300px;">
        <div class="card-block">
          <p class="card-text"><small class="text-muted">Uploaded <%= photo.created_at %></small></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
