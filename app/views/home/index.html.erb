<div class="row">
  <% events.each do |event| %>
    <div class="col-sm-6 col-md-4 p-2">
      <div class="card home-card" style="background-image: url('<%= PhotoService.url(event.photos.first.thumbnails.first) %>');">
        <div class="card-block">
          <p class="card-text"><small class="text-muted"><%= event.title %></small></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
